{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "username": {
      "$id": "/properties/username",
      "type": "string",
      "title": "The Username Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["hueter"]
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "minLength": 5,
      "maxLength": 55,
      "examples": ["foo123"]
    },
    "firstname": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Michael"]
    },
    "lastname": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Hueter"]
    },
    "address": {
      "$id": "/properties/address",
      "type": "string",
      "title": "The Address Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 85,
      "examples": ["123 Hardcore St"]
    },
    "city": {
      "$id": "/properties/city",
      "type": "string",
      "title": "The City Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["San Jose"]
    },
    "state": {
      "$id": "/properties/state",
      "type": "string",
      "title": "The State Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 65,
      "examples": ["California"]
    },
    "country": {
      "$id": "/properties/country",
      "type": "string",
      "title": "The Country Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 65,
      "examples": ["USA"]
    },
    "zipcode": {
      "$id": "/properties/state",
      "type": "integer",
      "title": "The zipcode Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 12,
      "examples": ["California"]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "minLength": 6,
      "maxLength": 60,
      "title": "The Email Schema ",
      "default": "",
      "examples": ["michael@rithmschool.com"]
    }
  },
  "required": ["firstname", "lastname","email", "username", "password"]
}
